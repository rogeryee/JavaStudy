<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person"
          class="com.yee.study.spring.framework.ioc.bean.Person"/>

    <bean id="myBeanFactoryPostProcessor"
          class="com.yee.study.spring.framework.ioc.bean.MyBeanFactoryPostProcessor"/>

    <bean id="myBeanDefinitionRegistryPostProcessor"
          class="com.yee.study.spring.framework.ioc.bean.MyBeanDefinitionRegistryPostProcessor"/>

</beans>